CREATE PROCEDURE [dbo].[mp_SurveyQuestions_Insert]

/*
Author:   			Rob Henry
Created: 			2007-09-18
Last Modified: 		2010-07-02
*/

@QuestionGuid uniqueidentifier,
@PageGuid uniqueidentifier,
@QuestionText nvarchar(max),
@QuestionTypeId int,
@AnswerIsRequired bit,
@ValidationMessage nvarchar(255)
	
AS

DECLARE @pageQuestionCount int

SELECT 
	@pageQuestionCount = Count(*) 
FROM mp_SurveyQuestions
WHERE PageGuid = @PageGuid 

INSERT INTO 	[dbo].[mp_SurveyQuestions] 
(
				[QuestionGuid],
				[PageGuid],
				[QuestionText],
				[QuestionTypeId],
				[AnswerIsRequired],
				[QuestionOrder],
				[ValidationMessage]
) 

VALUES 
(
				@QuestionGuid,
				@PageGuid,
				@QuestionText,
				@QuestionTypeId,
				@AnswerIsRequired,
				(@pageQuestionCount),
				@ValidationMessage
)

GO

