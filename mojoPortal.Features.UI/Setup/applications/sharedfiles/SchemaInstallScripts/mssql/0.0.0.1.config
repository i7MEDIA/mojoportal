CREATE PROCEDURE [dbo].[mp_SharedFiles_SelectByModule]

/*
Author:   			
Created: 			1/5/2005
Last Modified: 		2008-01-28

*/

@ModuleID		int,
@FolderID		int

AS
SELECT
		sf.[ItemID],
		sf.[ModuleID],
		sf.[UploadUserID],
		sf.[FriendlyName],
		sf.[OriginalFileName],
		sf.[ServerFileName],
		sf.[SizeInKB],
		sf.[UploadDate],
		sf.[FolderID],
		sf.ItemGuid,
		sf.FolderGuid,
		sf.UserGuid,
		sf.ModuleGuid,
		u.[Name] As UserName
		
FROM
		[dbo].[mp_SharedFiles] sf

LEFT OUTER JOIN
		mp_Users u
ON		sf.UploadUserID = u.UserID

WHERE	sf.ModuleID = @ModuleID
		AND sf.FolderID = @FolderID
GO
