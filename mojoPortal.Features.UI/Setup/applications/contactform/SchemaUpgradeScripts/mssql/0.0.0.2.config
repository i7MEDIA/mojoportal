CREATE PROCEDURE [dbo].[mp_ContactFormMessage_DeleteBySite]

/*
Author:   			
Created: 			2008-11-12
Last Modified: 		2008-11-12
*/

@SiteID int

AS

DELETE FROM [dbo].[mp_ContactFormMessage]
WHERE SiteGuid IN (SELECT SiteGuid FROM mp_Sites WHERE SiteID = @SiteID)


GO


