<?xml version="1.0"?>
<!-- 
you should not customize this file, you should create a copy with a different name and then configure to use your custom file
from this setting in Web.config <add key="TinyMCE:ConfigFile" value="mojoTinyMCE4.config" /> ideally overriding it in your user.config fle
By using a different file you will not lose your custom settings during upgrades
-->
<TinyMCE>
  
  <Editor 
    Name="Full" 
    Menubar="edit insert view format table tools"
    DisableMenuBar="false"
    Plugins="autosave,code,media,searchreplace,fullscreen,emoticons,directionality,table,image,link,anchor,wordcount,visualblocks,charmap"
    Toolbar1Buttons="code fullscreen visualblocks | selectall removeformat cleanup cut copy paste pastetext pasteword | print | undo redo searchreplace | bold italic underline strikethrough superscript"
    Toolbar2Buttons="formatselect styleselect blockquote | bullist numlist | outdent indent | alignleft aligncenter alignright alignjustify | link unlink anchor | image media table hr emoticons charmap"
    Toolbar3Buttons=""
    ExtendedValidElements="iframe[src|width|height|style|name|title|align|scrolling|frameborder|allowtransparency],meta[itmeprop|content],link[itemprop|href|class|onclick],a[itemprop|href|target|class|onclick]"
    ForcePasteAsPlainText="false"
    EnableObjectResizing="true"
    UnDoLevels="10"
	EnableImageAdvancedTab="true"
    >
  </Editor>
  
  <Editor
    Name="FullWithTemplates"
    Menubar="edit insert view format table tools"
