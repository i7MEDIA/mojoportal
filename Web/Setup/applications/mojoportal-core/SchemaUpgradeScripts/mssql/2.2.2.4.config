CREATE PROCEDURE [dbo].[mp_ForumSubscriptions_Exists]

/*
Author:			
Created:		2007/05/24
Last Modified:	2007/05/24

*/
    
@ForumID  	int,
@UserID		int

AS
IF EXISTS (	SELECT 	SubscriptionID
		FROM		mp_ForumSubscriptions
		WHERE	ForumID = @ForumID
				AND UserID = @UserID
				AND UnSubscribeDate is null )
SELECT 1
ELSE
SELECT 0
GO



