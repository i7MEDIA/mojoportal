CREATE PROCEDURE [dbo].[mp_PayPalLog_SelectByCart]

/*
Author:   			
Created: 			2008-07-01
Last Modified: 		2008--07-01
*/

@CartGuid uniqueidentifier

AS


SELECT *
		
FROM
		[dbo].[mp_PayPalLog]
		
WHERE
		[CartGuid] = @CartGuid

ORDER BY
		CreatedUtc

GO

