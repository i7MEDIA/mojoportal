CREATE PROCEDURE [dbo].[mp_PayPalLog_SelectNewestLog]

/*
Author:   			
Created: 			2008-06-26
Last Modified: 		2008-06-26
*/

@CartGuid uniqueidentifier,
@RequestType nvarchar(255)

AS


SELECT TOP 1 *
		
FROM
		[dbo].[mp_PayPalLog]
		
WHERE
		[CartGuid] = @CartGuid
		AND (RequestType = @RequestType OR @RequestType = '')

ORDER BY 
		CreatedUtc DESC

GO





