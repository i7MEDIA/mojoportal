SET @TotalPages = @TotalPages + 1

SELECT
		t1.*,
		'TotalPages' = @TotalPages
		
FROM
		[dbo].[mp_UserLocation] t1

JOIN			#PageIndex t2
ON			
		t1.[RowID] = t2.[RowID]
		
WHERE
		t2.IndexID > @PageLowerBound 
		AND t2.IndexID < @PageUpperBound
		
ORDER BY t2.IndexID

DROP TABLE #PageIndex
