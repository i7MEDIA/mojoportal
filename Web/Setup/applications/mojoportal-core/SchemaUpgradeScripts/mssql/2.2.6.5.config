CREATE PROCEDURE [dbo].[mp_GeoZone_SelectByCode]

/*
Author:   			
Created: 			2008-07-08
Last Modified: 		2008-07-08
*/

@CountryGuid uniqueidentifier,
@Code	nvarchar(255)

AS


SELECT TOP 1 *
		
FROM
		[dbo].[mp_GeoZone]
		
WHERE
		[CountryGuid] = @CountryGuid
		AND [Code] = @Code

GO




