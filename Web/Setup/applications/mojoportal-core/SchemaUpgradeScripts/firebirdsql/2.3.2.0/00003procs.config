SET TERM ^ ;
ALTER PROCEDURE MP_LETTERSENDLOG_INSERT (
    LETTERGUID Char(36),
    USERGUID Char(36),
	SUBSCRIBEGUID Char(36),
    EMAILADDRESS Varchar(100),
    UTC Timestamp,
    ERROROCCURRED Smallint,
    ERRORMESSAGE Blob sub_type 1 )
RETURNS (
    ROWID Integer )
AS
BEGIN
 ROWID = NEXT VALUE FOR mp_LetterSendLog_seq;

INSERT INTO 	MP_LETTERSENDLOG
(				
                ROWID,
                LETTERGUID,
                USERGUID,
				SUBSCRIBEGUID,
                EMAILADDRESS,
                UTC,
                ERROROCCURRED,
                ERRORMESSAGE
) 
VALUES 
(				
               :ROWID,
               :LETTERGUID,
               :USERGUID,
			   :SUBSCRIBEGUID,
               :EMAILADDRESS,
               :UTC,
               :ERROROCCURRED,
               :ERRORMESSAGE
);

END^
SET TERM ; ^
